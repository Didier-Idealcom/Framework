{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.0",
        "ext-blackfire": "^1.29",
        "astrotomic/laravel-translatable": "^11.3",
        "barryvdh/laravel-translation-manager": "^0.6",
        "bepsvpt/secure-headers": "^7.2",
        "blade-ui-kit/blade-icons": "^1.0",
        "cloudinary-labs/cloudinary-laravel": "^1.0",
        "conedevelopment/blade-filters": "^1.0",
        "darkaonline/l5-swagger": "^8.0",
        "doctrine/dbal": "^2.10",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "igaster/laravel-theme": "^2.0",
        "kris/laravel-form-builder": "^1.30",
        "laravel/framework": "^9.0",
        "laravel/helpers": "^1.1",
        "laravel/legacy-factories": "^1.1",
        "laravel/passport": "^10.0",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^3.2",
        "league/flysystem-aws-s3-v3": "^3.0",
        "maatwebsite/excel": "^3.0",
        "nwidart/laravel-modules": "^7.0",
        "spatie/laravel-permission": "^5.5",
        "yajra/laravel-datatables-oracle": "^9.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.0",
        "beyondcode/laravel-dump-server": "^1.0",
        "codeception/codeception": "^5.0.0-RC5",
        "codeception/module-asserts": "^3.0",
        "codeception/module-laravel": "^3.0",
        "codeception/module-phpbrowser": "^3.0",
        "codeception/module-rest": "^3.0",
        "codeception/module-webdriver": "^3.0",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.0",
        "spatie/laravel-ignition": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Modules\\": "Modules/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
